{
    "compilerOptions": {
        "target": "es6",
        "module": "commonjs",
        "strict": true,
        "noImplicitAny": true,
        "resolveJsonModule": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "downlevelIteration": true
    }
}
